            <form method="POST" action="/user/validatecode">
          <div class="container bg-light my-2 py-2">
            <div class="row">
              <div class="col-md-12">
                <h4 class="text-dark text-center">
                  <b>Confirm Your Order</b>
                </h4>
              </div>
            </div>
            <div class="row my-1">
              <div class="col-md-5">
                <h4 class="text-dark text-left m-0">
                  <b>Order ID</b>
                </h4>
              </div>
              <div class="col-md-7">
                <h4 class="text-dark text-left m-0">
                  <b>: <%= transaksi.id %></b>
                </h4>
              </div>
            </div>
            <div class="row my-1">
              <div class="col-md-5">
                <h4 class="text-dark text-left m-0">
                  <b>Tipe</b>
                </h4>
              </div>
              <div class="col-md-7">
                <h4 class="text-dark text-left m-0">
                  <b>: Top Up</b>
                </h4>
              </div>
            </div>
            <div class="row my-1">
              <div class="col-md-5">
                <h4 class="text-dark text-left m-0">
                  <b>Total</b>
                </h4>
              </div>
              <div class="col-md-7">
                <h4 class="text-dark text-left m-0">
                  <b>: Rp. <%= transaksi.total.replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1.") %></b>
                </h4>
              </div>
            </div>
            <div class="row">
              <div class="col-md-5">
                <h4 class="text-dark my-2 text-left">
                  <b>Enter This Code</b>
                </h4>
              </div>
              <div class="col-md-7">
                <h4 class="text-dark my-2 text-left">
                  <b>: <%= transaksi.confirmation_code %></b>
                </h4>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <input type="text" class="form-control" name="confirmation_code" placeholder="input your confirmation code">
              </div>
            </div>
            <div class="row">
              <div class="col-md-12 m-2">
                <button type="submit">Confirm</button>
              </div>
            </div>
          </div>
