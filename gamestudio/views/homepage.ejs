      <div class="row">
        <%  _.each(games,function(popular){ %>
        <div class="col-md-3">
          <a href="http://localhost:1337/games/detailGame/<%= popular.id %>"><img class="img-fluid d-block" src="<%= popular.photo_url %>"></a>
          <div class="row">
            <div class="col-md-12" style="height:35px;">
              <h6 class="py-1 text-left m-0 judul">
                <b><%= popular.game_name %></b>
              </h6>
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <h6 class="py-1 m-0 text-left genre" style="font-size:14px;">
                <%  _.each(popular.genreStrings,function(genre_list){ %>
                          <a href="#">
                            <%= genre_list.nama_genre %>,
                          </a>
                <%})%>
              </h6>
            </div>
            <div class="col-md-6 text-right">
              <i class="fa fa-star" style="font-size:14px;"></i>
              <i class="fa fa-star" style="font-size:14px;"></i>
              <i class="fa fa-star" style="font-size:14px;"></i>
              <i class="fa fa-star" style="font-size:14px;"></i>
              <i class="fa fa-star" style="font-size:14px;"></i>
